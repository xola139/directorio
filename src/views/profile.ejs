<html>
  <body>
	<% if (!user) { %>
		<p>Welcome! Please <a href="/login">log in</a>.</p>
	<% } else { %>
		<p>Hello, <%= user.username %>. View your <a href="/profile">profile</a>.</p>
	<% } %>

	<script>
    localStorage.setItem('usuario', '<%= user.username %>');
    var tmp = '<%= user.username %>';
    if(tmp == 'escortenmx')
       tmp = 'sopitas';
		window.location.href = "/#/perfil/"+tmp;
	</script>



	<form >
        <div class="form-group">
          <label for="name">Telefono</label>
          <input type="text" class="form-control"  name="telefno" required>
        </div>
       
        <div class="form-group">
          <button type="submit" class="btn btn-success" [disabled]="!bookForm.form.valid">Update</button>
        </div>
    </form>


 </body>
</html>